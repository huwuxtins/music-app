<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MusicApp"
        tools:targetApi="31">
        <activity
            android:name=".FinalRegister"
            android:exported="false"
            android:label="@string/title_activity_final_register"
            android:theme="@style/Theme.MusicApp" />
        <activity
            android:name=".activities.RegisterPassword"
            android:exported="true" />
        <activity
            android:name=".activities.RegisterEmail"
            android:exported="true" />
        <activity
            android:name=".activities.EnterLogin"
            android:exported="true"
            android:parentActivityName=".activities.Login" />
        <activity
            android:name=".activities.Login"
            android:exported="true" />
        <activity
            android:name=".activities.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>